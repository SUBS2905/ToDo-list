<%- include("header.ejs") %> 

    <div class="box" id="heading">
        <h1> <%= listTitle %> </h1> 
    </div>

    <div class="box">
        <% for(var i = 0;i<task.length;i++){ %> 
            <div class="item">
                <input type="checkbox">
                <p><%= task[i] %> </p>
            </div> 
        <% } %> 
        <form class="item" action="/" method="post">
            <input name="task" type="text" placeholder="New Task..." autocomplete="off">
            <button type="submit" name="list" value=<%= listTitle %>>+</button>
        </form>
    </div>
    
<%- include("footer.ejs") %> 